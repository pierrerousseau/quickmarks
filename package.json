{
  "name": "quickmarks",
  "version": "1.0.0",
  "engines": [
    "node = 0.8.x"
  ],
  "description": "Manage your bookmark from your Cozy!",
  "repository": {
    "type": "git",
    "url": "https://github.com/pierrerousseau/quickmarks.git"
  },
  "main": "server.coffee",
  "dependencies": {
    "americano": "0.3.11",
    "americano-cozy": "0.2.11"
  },
  "devDependencies": {
    "should": "= 1.2.0",
    "mocha": "= 1.6.0",
    "chai": "= 1.3.0"
  },
  "scripts": {
    "test": "cake tests",
    "start": "node server.js"
  },
  "cozy-displayName": "quickmarks",
  "cozy-permissions": {
    "bookmark": {
      "description": "Allow you to manage your bookmarks in a simple way"
    }
  }
}
